AE,QAAQ,EAAE,GAAG,CAAC,IAAI,CAAC,QAAQ,EAAE,CAAC;IACpE,MAAM,MAAM,GAAG,EAAE,KAAK,EAAE,GAAG,CAAC,MAAM,CAAC,GAAG,EAAE,CAAC;IAEzC,MAAM,MAAM,GAAG,iBAAS,CAAC,SAAS,CAAC,MAAM,EAAE,IAAI,CAAC;SAC3C,IAAI,CAAC,IAAI,CAAC,EAAE,CAAC,IAAI,CAAC;SAClB,KAAK,CAAC,GAAG,CAAC,EAAE;QACT,OAAO,IAAI,CAAC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC;IAC5B,CAAC,CAAC,CAAC;IACP,OAAO,MAAM,CAAC;AAClB,CAAC,CAAA;AAVY,QAAA,UAAU,cAUtB;AAEM,MAAM,UAAU,GAAG,CAAC,GAAW,EAAE,GAAY,EAAE,IAAiB,EAAE,EAAE;IACvE,MAAM,MAAM,GAAG,iBAAS,CAAC,SAAS,CAAC,EAAE,KAAK,EAAE,GAAG,CAAC,MAAM,CAAC,GAAG,EAAE,CAAC;SACxD,IAAI,CAAC,IAAI,CAAC,EAAE,CAAC,IAAI,CAAC;SAClB,KAAK,CAAC,GAAG,CAAC,EAAE;QACT,OAAO,IAAI,CAAC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC;IAC5B,CAAC,CAAC,CAAC;IACP,OAAO,MAAM,CAAC;AAElB,CAAC,CAAA;AARY,QAAA,UAAU,cAQtB","sourcesContent":["import { NextFunction, Request, Response } from 'express';\nimport { Schema, model, Date } from 'mongoose';\n\ninterface User {\n    email: string;\n    password?: string;\n    lastLogin: Date;\n    token: string\n}\n  \nconst userSchema = new Schema<User>({\n\n    email:  { \n        type: String, \n        index: true, \n        unique: true,\n        required: true\n    },\n    password: {\n        type: String, \n        required: true\n    },\n    lastLogin : {\n        type : Date,\n        default: Date.now()\n    },\n    token: {\n        type : String\n    }\n\n});\n\nexport const userModel = model<User>('users', userSchema);\n\nexport const insertUser =  (req:any, res:Response, next:NextFunction) => {\n    const data = { email: req.body.email, password: req.body.password };\n    const userData = new userModel(data);\n\n    const result = userData.save()\n        .then(data => data)\n        .catch(err => {\n            const error = new Error(err);\n            error.message = \"There is some issue in creating this account\";\n                \n            if (err.index === 0) { \n                error.message = \"User already exists\";\n            }                        \n            return next(error);\n        });\n\n    return result;\n\n}\n    \nexport const fetchUser = () => {\n    const result = userModel.find({})\n        .then(data => data)       \n        .catch(err => err)\n    return result;\n}\n    \nexport const fetchUserByEmail = (req:Request ,res:Response, next:NextFunction) => {\n    const result = userModel.find({ email: req.params.key })\n        .then(data => data)       \n        .catch(err => {\n            return next(Error(err));\n        });\n    return result;\n}\n    \nexport const updateUser = (req:Request, res:Response, next:NextFunction) => {\n    const data = { email: req.body.email, password: req.body.password };                \n    const filter = { email: req.params.key };\n    \n    const result = userModel.updateOne(filter, data)\n        .then(data => data)       \n        .catch(err => {\n            return next(Error(err));\n        });\n    return result;        \n}\n    \nexport const deleteUser = (req:Request, res:Response, next:NextFunction) => {\n    const result = userModel.deleteOne({ email: req.params.key })\n        .then(data => data)       \n        .catch(err => {\n            return next(Error(err));\n        });\n    return result;        \n\n}    \n"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"c91e3ebcd271af717ba4cb7aeb9eb1e69a5db044","contentHash":"cf34b3fb2a1c8092879bc886adc393570461f0d4e062d2fbf86ff0b400b3ddb8"}}